{
  "welcomeFile": "/UI5-SAMPLE/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/UI5-SAMPLE/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
		"source": "^/CAP_NODE/(.*)",
		"target": "$1",
		"destination": "CAP_NODE",
		"csrfProtection": false
	}
  ]
}